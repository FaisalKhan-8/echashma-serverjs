{
  "name": "echashma-server",
  "version": "1.0.0",
  "main": "src/server.js",
  "bin": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "babel src --out-dir dist",
    "pkg":"pkg . --out build/echashma.exe"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "jsonwebtoken": "^9.0.2",
    "pkg": "^5.7.0",
    "portfinder": "^1.0.32",
    "zod": "^3.23.8"
  },
  "pkg": {
  "scripts": ["dist/**/*.js"],
  "assets": [
    "src/dist/**/*",
    ".env",
    "node_modules/.prisma/**/*",
    "node_modules/@prisma/client/runtime/**/*.node"
  ],
  "output": "./build/echashma",
  "targets": [
    "node16-win-x64"
  ]
},

  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4"
  }
}
